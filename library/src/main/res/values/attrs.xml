<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="FillImageView">
        <attr name="fiv_normalColor" format="color"/>
        <attr name="fiv_fillColor" format="color" />
        <attr name="fiv_drawable" format="reference" />
        <attr name="fiv_from" format="enum">
            <enum name="left" value="1" />
            <enum name="top" value="2" />
            <enum name="right" value="4" />
            <enum name="bottom" value="8" />
        </attr>
        <attr name="fiv_percent" format="float" />
    </declare-styleable>

</resources>